package core;

public class P003_TYpeCasting {
	public static void main(String[] args) {
		// -128 to 127
		byte b = 127; //1
		
		// -32768 to 32767
		short s  = b; //2 -> implicit
		
		int i = 10, j = 3;//4
		int k = i / j;
		System.out.println(k);
		
		double d = i/j;
		System.out.println(d);
		
	}
}
