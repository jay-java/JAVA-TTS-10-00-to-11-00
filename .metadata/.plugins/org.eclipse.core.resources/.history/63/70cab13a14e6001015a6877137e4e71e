package core;

import java.util.ArrayList;
import java.util.List;

class Employee {
	int id;
	String name;
	double salary;

	Employee(int id, String name, double salary) {
		this.id = id;
		this.name = name;
		this.salary = salary;
	}

	@Override
	public String toString() {
		return "Employee : id - " + id + " name - " + name + " salary - " + salary + "";
	}
}

public class P023_ListPractical {
	public static void main(String[] args) {
		Employee e1= new Employee(1, "java", 987546.011);
		Employee e2= new Employee(2, "php", 7546.011);
		Employee e3= new Employee(3, "python", 97546.011);
		Employee e4= new Employee(4, "DS", 98756.011);
		Employee e5= new Employee(5, "Flutter", 187546.011);
		
		List<Employee> list  = new ArrayList<Employee>();
		list.add(e1);
		list.add(e2);
		list.add(e3);
		list.add(e4);
		list.add(e5);
		System.out.println(list);
		
		
		
	}
}
