package core;

abstract class RBI {
	public abstract void SA();// abstract function

	public abstract void HL();

	public void show() {
		System.out.println("show parent RBI class");
	}
	
	public static void repoRate() {
		System.out.println("repo Rate : +-4%");
	}
}

class SBI extends RBI {

	@Override
	public void SA() {
		// TODO Auto -generated method stub
		System.out.println("SBI interest rate : 5%");
	}

	@Override
	public void HL() {
		// TODO Auto-generated method stub
		System.out.println("SBI HL : 6%");
	}

}

class HDFC extends RBI {

	@Override
	public void SA() {
		// TODO Auto-generated method stub
		System.out.println("HDFC interest rate : 6%");
	}

	@Override
	public void HL() {
		// TODO Auto-generated method stub
		System.out.println("HDFC HL : 7%");
	}

}

class JAVA extends RBI {

	@Override
	public void SA() {
		// TODO Auto-generated method stub
		System.out.println("JAVA interest rate : 7%");
	}

	@Override
	public void HL() {
		// TODO Auto-generated method stub
		System.out.println("JAVA HL : 9%");
	}

}

public class P016_Abstrction {
	public static void main(String[] args) {
		SBI s = new SBI();
		s.SA();
		s.HL();
		s.show();

		HDFC h = new HDFC();
		h.SA();
		h.HL();
		h.show();

		JAVA j = new JAVA();
		j.SA();
		j.HL();
		j.show();
		
		SBI.repoRate();
		HDFC.repoRate();
		JAVA.repoRate();
	}
}
