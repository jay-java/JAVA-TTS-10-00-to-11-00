package core;

//1.Predefine
//2.User define
class Functions {
	//1. Without return type without parameter
	public void run() {
		System.out.println("run method");
	}
	//2. Without return type with parameter/argument
	public void call(int i) {
		System.out.println("call without return type and with para : "+i);
	}
	
	//3. With return type without parameter
	public byte fun() {//-
		System.out.println("with return type without para");
		return 987653;
	}
	//4. With return type with parameter

}

public class P010_Functions {
	public static void main(String[] args) {
		Functions f= new Functions();
		f.run();
		f.call(12);
		
	}
}
