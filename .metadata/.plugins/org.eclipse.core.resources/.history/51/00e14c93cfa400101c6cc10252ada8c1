package core;

public class P005_Loops {
	public static void main(String[] args) {

		// 1.for(initialization;condition;incre/decre){}

		for (int i = 1; i <= 10; i++) {
			System.out.println(i);
		}
		for (int i = 10; i >= 1; i--) {
			System.out.println(i);
		}

		// 2.while(){}
		int j = 1;
		while(j <= 10) {
			System.out.println(j);
		}
		// 3. do {} while(condition);
		// 4.for each
	}
}
