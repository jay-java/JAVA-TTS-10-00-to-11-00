package core;

public class P007_Patterns {
	public static void main(String[] args) {
		for (int i = 1; i <= 3; i++) { // row
			for (int j = 1; j <= 3; j++) { // col
				System.out.print("* ");
			}
			System.out.println();
		}

		System.out.println("============");
		
		for (int i = 1; i <= 3; i++) { // row
			for (int j = 1; j <= 3; j++) { // col
				System.out.print("* ");
			}
			System.out.println();
		}
	}
}
