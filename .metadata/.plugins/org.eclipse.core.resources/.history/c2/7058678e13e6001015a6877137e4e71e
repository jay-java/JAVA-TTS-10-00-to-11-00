package core;

class Employee {
	int id;
	String name;
	double salary;

	Employee(int id, String name, double salary) {
		this.id = id;
		this.name = name;
		this.salary = salary;
	}

	@Override
	public String toString() {
		return "Employee : id - " + id + " name - " + name + " salary - " + salary + "";
	}
}

public class P023_ListPractical {
	public static void main(String[] args) {
		
	}
}
