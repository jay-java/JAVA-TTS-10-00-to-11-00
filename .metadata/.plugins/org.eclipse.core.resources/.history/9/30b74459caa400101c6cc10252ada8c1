package core;

public class P004_ConditionalStatements {
	public static void main(String[] args) {
		int i = 3, j = 2;
		// 1.if
		if (i > j) {
			System.out.println("i is greater than j");
		}

		// 2.if else
		if (i > j) {
			System.out.println("i is greater than j");
		} else {
			System.out.println("j is greater than i");
		}

		// 3.nested if
		int age = 36;
		if(age > 18) {
			if(age < 55) {
				
			}
		}
		
		else {
			System.out.println("age is less than 18");
		}
	}
}
