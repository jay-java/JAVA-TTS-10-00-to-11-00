package core;

import java.util.Scanner;

//1.Predefine
//2.User define
class Functions {
	//1. Without return type without parameter
	public void run() {
		System.out.println("run method");
	}
	//2. Without return type with parameter/argument
	public void call(int i) {
		System.out.println("call without return type and with para : "+i);
	}
	
	//3. With return type without parameter
	public int fun() {//-
		System.out.println("with return type without para");
		return 9876;
	}
	//4. With return type with parameter
	public int getSquare(int num) {
		System.out.println("number is coming from main method : "+num);
		return num*num;
	}
}

public class P010_Functions {
	public static void main(String[] args) {
		Functions f= new Functions();
		f.run();
		f.call(12);
		
		int res = f.fun();
		System.out.println(res);
		
		Scanner sc = new Scanner(System.in);
		System.out.println("enter number to get sqaure : ");
		int num = sc.nextInt();
		
		int squaer = f.getSquare(num);
		
	}
}
