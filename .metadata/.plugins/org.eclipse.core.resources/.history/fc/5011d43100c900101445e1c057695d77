package core;

abstract class RBI {
	public abstract void SA();// abstract function

	public abstract void HL();
}

class SBI extends RBI {

	@Override
	public void SA() {
		// TODO Auto-generated method stub
		System.out.println("SBI interest rate : 5%");
	}

	@Override
	public void HL() {
		// TODO Auto-generated method stub

	}

}

class HDFC extends RBI {

	@Override
	public void SA() {
		// TODO Auto-generated method stub
		System.out.println("SBI interest rate : 6%");
	}

	@Override
	public void HL() {
		// TODO Auto-generated method stub

	}

}

class JAVA extends RBI {

	@Override
	public void SA() {
		// TODO Auto-generated method stub
		System.out.println("SBI interest rate : 7%");
	}

	@Override
	public void HL() {
		// TODO Auto-generated method stub

	}

}

public class P014_Abstrction {
	public static void main(String[] args) {

	}
}
