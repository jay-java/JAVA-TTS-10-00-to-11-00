package core;

class Student {
	int id;
	String name;
	String address;
	long contac;

	public void setId(int id) {
		this.id = id;
	}

	public int getId() {
		return id;
	}

	public void setName(String name) {
		this.name = name;

	}

	public String getName() {
		return name;
	}

	public void setAddress(String address) {
		this.address = address;
	}

}

public class P016_Encapsulation {
	public static void main(String[] args) {

	}
}
