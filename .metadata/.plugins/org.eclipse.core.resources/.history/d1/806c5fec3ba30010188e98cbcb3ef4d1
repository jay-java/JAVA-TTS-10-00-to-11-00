package core;

public class P003_Operators {
	public static void main(String[] args) {
		// 1. Arithmetic -> +,-,*,/,%
		System.out.println(1 + 2);
		System.out.println(1 - 2);
		System.out.println(1 * 2);
		System.out.println(1 / 2);
		System.out.println(10 % 3);

		// 2.Assignment -> =,+=,-=,/=,*=
		int i = 12;
		System.out.println(i);

		int j = 12;
		System.out.println("j = " + j);

		j += 2; // j = j+2
		System.out.println(j); // 14

		j += i; // j = j+i
		System.out.println(j);
		
		j -= i; //j = j-i
		System.out.println(j);

	}
}
